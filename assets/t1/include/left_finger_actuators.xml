<!-- Left Inspire Hand Finger Actuators (Optional)
     Include this in the <actuator> block to enable finger control.
     12 joints total: 4 thumb + 2 index + 2 middle + 2 ring + 2 little
-->
<motor name="left_thumb_1" joint="left_thumb_1_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_thumb_2" joint="left_thumb_2_joint" ctrlrange="-2 2" ctrllimited="true"/>
<motor name="left_thumb_3" joint="left_thumb_3_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_thumb_4" joint="left_thumb_4_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_index_1" joint="left_index_1_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_index_2" joint="left_index_2_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_middle_1" joint="left_middle_1_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_middle_2" joint="left_middle_2_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_ring_1" joint="left_ring_1_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_ring_2" joint="left_ring_2_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_little_1" joint="left_little_1_joint" ctrlrange="-1 1" ctrllimited="true"/>
<motor name="left_little_2" joint="left_little_2_joint" ctrlrange="-1 1" ctrllimited="true"/>

